            print(balants)
        #################################################################
        #################################################################
        #################################################################
        elif g != 0 and g >= sum_stav and kol_stavok == 0 or kol_stavok <= 0:
            e1 = input("Hotite sigrat -")
            if e1 == "Da":
                Game(g, balants)
            elif e1 == "No":
                print(balants)


        else:
            kol_stavok -= 1
            balants -= sum_stav
            g -= sum_stav  ##################
            print("Losse")

            print(d[0], d[1], d[2], d[3], d[4])
            print(d1[0], d1[1], d1[2], d1[3], d1[4])
