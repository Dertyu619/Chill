            print(balants)
        elif d2[0] == 3 and d1[1] == 3 and d1[2] == 3 and d1[3] == 3 and kol_stavok >= 0:  # d2 d1 d1 d1
            kol_stavok -= 1
            balants += sum_stav * 15
            g += sum_stav * 15  ##################
            print("You Vin")

            print(d[0], d[1], d[2], d[3], d[4])
            print(d1[0], d1[1], d1[2], d1[3], d1[4])
            print(d2[0], d2[1], d2[2], d2[3], d2[4])

            print(kol_stavok)
            print(balants)
        #################################################################1
        elif d1[0] == 3 and d1[1] == 3 and d1[2] == 3 and d1[3] == 3 and d1[
            4] == 3 and kol_stavok >= 0:  # d1 d1 d1 d1 d1
            kol_stavok -= 1
            balants += sum_stav * 45
            g += sum_stav * 45  ##################
            print("You Vin")

            print(d[0], d[1], d[2], d[3], d[4])
            print(d1[0], d1[1], d1[2], d1[3], d1[4])
            print(d2[0], d2[1], d2[2], d2[3], d2[4])

            print(kol_stavok)
            print(balants)
        elif d[0] == 3 and d1[1] == 3 and d1[2] == 3 and d1[3] == 3 and d1[4] == 3 and kol_stavok >= 0:  # d d1 d1 d1 d1
            kol_stavok -= 1
            balants += sum_stav * 45
            g += sum_stav * 45  ##################
            print("You Vin")

            print(d[0], d[1], d[2], d[3], d[4])
            print(d1[0], d1[1], d1[2], d1[3], d1[4])
            print(d2[0], d2[1], d2[2], d2[3], d2[4])

