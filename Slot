from random import randint

g = 0
balants = int(input())

def Game( g, balants ):
    sum_stav = int(input())
    kol_stavok = int(input())

    d = [[], [], [], [], []]
    d1 = [[], [], [], [], []]
    d2 = [[], [], [], [], []]

    while balants != 0 and kol_stavok >= 0:
        d[0] = randint(1, 3)
        d[1] = randint(1, 3)
        d[2] = randint(1, 3)
        d[3] = randint(1, 3)
        d[4] = randint(1, 3)

        d1[0] = randint(1, 3)
        d1[1] = randint(1, 3)
        d1[2] = randint(1, 3)
        d1[3] = randint(1, 3)
        d1[4] = randint(1, 3)

        d2[0] = randint(1, 3)
        d2[1] = randint(1, 3)
        d2[2] = randint(1, 3)
        d2[3] = randint(1, 3)
        d2[4] = randint(1, 3)
        #################################################################0
        if d[0] == 3 and d[1] == 3 and d[2] == 3 and kol_stavok >= 0:  # d d d
            kol_stavok -= 1
            balants += sum_stav * 15
            g += sum_stav * 15##################
            print("You Vin")

            print(d[0], d[1], d[2], d[3], d[4])
            print(d1[0], d1[1], d1[2], d1[3], d1[4])
            print(d2[0], d2[1], d2[2], d2[3], d2[4])

            print(kol_stavok)
            print(balants)
        if d1[0] == 3 and d[1] == 3 and d[2] == 3 and kol_stavok >= 0:  # d1 d d
            kol_stavok -= 1
            balants += sum_stav * 5
            g += sum_stav * 5  ##################
            print("You Vin")

            print(d[0], d[1], d[2], d[3], d[4])
            print(d1[0], d1[1], d1[2], d1[3], d1[4])
            print(d2[0], d2[1], d2[2], d2[3], d2[4])

            print(kol_stavok)
            print(balants)
        #################################################################0
        elif d[0] == 3 and d[1] == 3 and d[2] == 3 and d[3] == 3 and kol_stavok >= 0:  # d d d d
            kol_stavok -= 1
            balants += sum_stav * 15
            g += sum_stav * 15  ##################
            print("You Vin")

            print(d[0], d[1], d[2], d[3], d[4])
            print(d1[0], d1[1], d1[2], d1[3], d1[4])
            print(d2[0], d2[1], d2[2], d2[3], d2[4])

            print(kol_stavok)
            print(balants)
        elif d1[0] == 3 and d[1] == 3 and d[2] == 3 and d[3] == 3 and kol_stavok >= 0:  # d1 d d d
            kol_stavok -= 1
            balants += sum_stav * 15
            g += sum_stav * 15  ##################
            print("You Vin")

            print(d[0], d[1], d[2], d[3], d[4])
            print(d1[0], d1[1], d1[2], d1[3], d1[4])
            print(d2[0], d2[1], d2[2], d2[3], d2[4])

            print(kol_stavok)
            print(balants)
