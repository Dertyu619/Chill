        elif d[0] == 3 and d[1] == 3 and d[2] == 3 and d[3] == 3 and d[4] == 3 and kol_stavok >= 0:  # d d d d d
            kol_stavok -= 1
            balants += sum_stav * 45
            g += sum_stav * 45  ##################
            print("You Vin")

